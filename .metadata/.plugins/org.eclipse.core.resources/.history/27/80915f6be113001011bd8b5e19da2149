package practiceCode;

public class SumOfAnArray {

	public static void main(String[] args) {
		int[] arr= {0,2,4,6,8,10};
		
		
		System.out.println(SumOfAnArray.sum(arr));
	}
	
	public static int sum(int arr[]) {
		int sum=0;
		
		for(int i=0;i<arr.length;i++) {
			sum=sum+arr[i];
		}
		
		return sum;
	}

}
