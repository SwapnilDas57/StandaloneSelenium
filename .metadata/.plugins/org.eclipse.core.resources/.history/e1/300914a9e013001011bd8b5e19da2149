package practiceCode;

public class CheckPrimeNumber {

	public static void main(String args[]) {
		
	}
	
	public static boolean checkPrime(int number) {
		boolean isPrime=true;
		
		for(int i=2;i<number/2;i++) {
			if(number%i==0) {
				isPrime=false;
				break;
			}
		}
		
		return isPrime;
	}
	
}
